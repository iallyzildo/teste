@model IEnumerable<ClienteMVC.Entity.TipoEndereco>

<table>
    <tr>
        <th>
            Descrição
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descricao)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdTipoEndereco }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdTipoEndereco })
        </td>
    </tr>
}
</table>
